import io;

class ArithmeticPrecedence {

    public static void main(String[] args) {
        int a; int b; int c; int result;

        a = 2;
        b = 3;
        c = 4;

        // sem parêntesis: b * c tem precedência sobre +
        result = a + b * c;       // 2 + (3*4) = 14
        io.print(result);

        // forçando parêntesis
        result = (a + b) * c;     // (2+3)*4 = 20
        io.print(result);

        // subtração, divisão e multiplicação mistas
        result = a * b - c / a;   // (2*3) - (4/2) = 6 - 2 = 4
        io.print(result);
    }
}
